<Application value="ParaBank">
    <Page name="LoginPage">
        <Element name="UserName" type="TextBox" locator="xpath" selector="//input[@type='text' and @name='username']"/>
        <Element name="PassWord" type="password" locator="xpath" selector="//input[@type='password' and @name='password']"/>
        <Element name="Submit" type="Button" locator="xpath" selector="//input[@type='submit' and @class='button']"/>
        <Element name="Error" type="Error" locator="xpath" selector="//p[@class='error']"/>
    </Page>

    <Page name="OpenNewAccount">
        <Element name="AccountType" type="Select" locator="xpath" selector="//select[@id='type' and @class='input ng-pristine ng-untouched ng-valid ng-not-empty']"/>
        <Element name="FromAccount" type="Select" locator="xpath" selector="//select[@id='fromAccountId']"/>
        <Element name="Submit" type="Button" locator="xpath" selector="//input[@type='submit' and @class='button']"/>
        <Element name="NewAccountID" type="href" locator="xpath" selector="//a[@id='newAccountId' and contains(@href,'activity.htm?id=')]"/>
        <Element name="AccountId" type="td" locator="xpath" selector="//td[@id='accountId' and @class='ng-binding']"/>
        <Element name="AcctType" type="td" locator="xpath" selector="//td[@id='accountType' and @class='ng-binding']"/>
        <Element name="Transactions" type="td" locator="xpath" selector="//a[contains(@href,'transaction.htm?id') and @class='ng-binding']"/>
    </Page>

    <Page name="BillPay">
    <Element name="ZipCode" type="TextBox" locator="xpath" selector="//input[@ng-model='payee.address.zipCode' and @name='payee.address.zipCode']"/>
    <Element name="PayeeAcct" type="TextBox" locator="xpath" selector="//input[@ng-model='payee.accountNumber' and @name='payee.accountNumber']"/>
    <Element name="VerifyAcct" type="TextBox" locator="xpath" selector="//input[@ng-model='verifyAccount' and @name='verifyAccount']"/>
        <Element name="Amount" type="TextBox" locator="xpath" selector="//input[@ng-model='amount' and @name='amount']"/>
        <Element name="FromAcctID" type="TextBox" locator="xpath" selector="//select[@name='fromAccountId']"/>
        <Element name="phone" type="TextBox" locator="xpath" selector="//input[@name='payee.phoneNumber']"/>
        <Element name="Submit" type="Button" locator="xpath" selector="//input[@type='submit' and @value='Send Payment']"/>


    </Page>
</Application>